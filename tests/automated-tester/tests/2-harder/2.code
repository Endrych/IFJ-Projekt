' vypise vsechna suda cisla od 0 do 100

function print_even_to(count as integer) as integer
    if count >= 0 then
    
    else 
        return 1
    end if
    
    dim num as integer = 0
    do while num <= count
        print num;
        num = num + 2
    loop
end function

scope 
    dim return_value as integer
    return_value = print_even_to(100)
    if return_value <> 0 then
        print !"Nastala chyba\n";
    else 
        print !"\n";
    end if
end scope
